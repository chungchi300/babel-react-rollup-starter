{
  "name": "rollup-babel-react-redux-lib-boilerplate",
  "version": "1.0.0",
  "title": "rollup-babel-react-redux-lib-boilerplate",
  "description": "A opinioned boilerplate for develop react component(with styled-components) and redux action,reducer with proper development tools",
  "keywords": [
    "Babel",
    "Browsersync",
    "React",
    "ES6",
    "TreeShaking",
    "Library",
    "Rollup",
    "StandardJS"
  ],
  "homepage": "https://github.com/chungchi300/rollup-babel-react-redux-lib-boilerplate",
  "author": {
    "name": "Jeff Chung",
    "url": "https://github.com/chungchi300"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chungchi300/rollup-babel-react-redux-lib-boilerplate"
  },
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=6.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "styled-components": "^2.1.1",
    "ua-parser-js": "^0.7.13",
    "urijs": "^1.18.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "browser-sync": "2.18.12",
    "flow-bin": "^0.33.0",
    "jest": "^18.1.0",
    "npm-run-all": "4.0.2",
    "opener": "^1.4.3",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.3.2",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "3.2.2",
    "eslint": "^3.3.1",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-promise": "^2.0.1"
  },
  "scripts": {
    "lint": "eslint src --quiet --cache && echo \"eslint: no lint errors\"",
    "build": "rollup -c make/prod.js",
    "build:dev": "rollup -c make/dev.js -w",
    "browse": "browser-sync start --s --index 'html/index-dev.html' --files 'html/**/*.html, build/**/*.js' --no-notify",
    "start": "npm-run-all --parallel build:dev browse",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "preversion": "git pull && npm up && npm run std --silent",
    "version": "npm run build && git add -A .",
    "postversion": "git push --tags origin HEAD"
  },
  "jest": {
    "modulePaths": [
      "./node_modules",
      "./src/common"
    ]
  },
  "standard": {
    "ignore": [
      "build"
    ]
  }
}
